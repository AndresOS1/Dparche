<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo2"
    tools:context=".Entrada">


    <TextView
        android:id="@+id/titulo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="casual"
        android:textColor="@color/azull"
        android:text="SELECCIONA TUS INTERESES"
        android:textSize="20dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="25dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="67dp"
        tools:ignore="MissingConstraints">


        <ImageView
            android:id="@+id/imageciclismo"
            android:layout_width="120dp"
            android:layout_height="100dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="50dp"
            app:srcCompat="@drawable/ciclismo" />

        <ImageView
            android:id="@+id/imageclub"
            android:layout_width="120dp"
            android:layout_height="100dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="50dp"
            app:srcCompat="@drawable/club" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="67dp"
        tools:ignore="MissingConstraints">

    </LinearLayout>

    <TextView
        android:id="@+id/txtfuria"
        android:layout_width="208dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Club"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="217dp" />

    <TextView
        android:id="@+id/txtsiclismo"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:fontFamily="monospace"
        android:text="Ciclismo"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="217dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="67dp"
        tools:ignore="MissingConstraints">


    </LinearLayout>




    <TextView
        android:id="@+id/txtambiente"
        android:layout_width="208dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Ambiente"
        android:fontFamily="monospace"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="356dp" />

    <TextView
        android:id="@+id/txtbar"
        android:layout_width="127dp"
        android:layout_height="wrap_content"
        android:text="Cofee bar"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="356dp"
        android:fontFamily="monospace"/>

    <ImageView
        android:id="@+id/imagepintura"
        android:layout_width="120dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/pintura"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="386dp" />

    <ImageView
        android:id="@+id/imageambiente"
        android:layout_width="120dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/ambiente"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="246dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/picinas"
        android:layout_width="208dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Piscinas"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="494dp"
        android:fontFamily="monospace"/>

    <TextView
        android:id="@+id/txtPinturas"
        android:layout_width="127dp"
        android:layout_height="wrap_content"
        android:text=" Pintura"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="494dp"
        android:fontFamily="monospace"/>

    <ImageView
        android:id="@+id/imagecofeebar"
        android:layout_width="120dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/bar"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="246dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imagepiscinas"
        android:layout_width="120dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/picinas"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="386dp" />


</androidx.constraintlayout.widget.ConstraintLayout>