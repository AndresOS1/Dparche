    <fragment
        android:id="@+id/nav_cul"
        android:name="com.example.deplegable.ui.Cultura.CulturaFragment"
        android:label="@string/menu_cul"
        tools:layout="@layout/fragment_cultura" />

    <fragment
        android:id="@+id/nav_recre"
        android:name="com.example.deplegable.ui.Recreacion.RecreacionFragment"
        android:label="@string/menu_recre"
        tools:layout="@layout/fragment_recre" />

    <fragment
        android:id="@+id/nav_motel"
        android:name="com.example.deplegable.ui.Moteles.MotelFragment"
        android:label="@string/menu_motel"
