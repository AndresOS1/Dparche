<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.deplegable.ui.home.HomeFragment"
        android:label="@string/menu_restaurantes"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_bar"
        android:name="com.example.deplegable.ui.gallery.GalleryFragment"
        android:label="@string/menu_bares"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_disco"
        android:name="com.example.deplegable.ui.slideshow.SlideshowFragment"
